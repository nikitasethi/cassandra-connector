<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:ns2="http://www.mulesoft.org/schema/mule/devkit" xmlns:mule="http://www.mulesoft.org/schema/mule/core" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.mulesoft.org/schema/mule/cassandradb" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace"></xs:import>
  <xs:import namespace="http://www.springframework.org/schema/beans" schemaLocation="http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/core" schemaLocation="http://www.mulesoft.org/schema/mule/core/current/mule.xsd"></xs:import>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-extension" name="config">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation></xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element type="mule:poolingProfileType" minOccurs="0" name="connection-pooling-profile">
              <xs:annotation>
                <xs:documentation>Characteristics of the connection pool.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="mule:abstract-reconnection-strategy">
              <xs:annotation>
                <xs:documentation>Reconnection strategy that defines how Mule should handle a connection failure.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>Give a name to this configuration so it can be later referenced by config-ref.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="localhost" name="host">
            <xs:annotation>
              <xs:documentation>Host name or IP address</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="9042" name="port">
            <xs:annotation>
              <xs:documentation>Port (default is 9042)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="" name="keyspace">
            <xs:annotation>
              <xs:documentation>Cassandra keyspace</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="clusterName">
            <xs:annotation>
              <xs:documentation>Cassandra cluster name</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/cassandradb" type="ProtocolVersionEnumType" use="optional" name="protocolVersion">
            <xs:annotation>
              <xs:documentation>Version of the native protocol supported by the driver.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/cassandradb" type="integerType" use="optional" name="maxSchemaAgreementWaitSeconds">
            <xs:annotation>
              <xs:documentation>The maximum time to wait for schema agreement before returning from a DDL query.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/cassandradb" type="CompressionEnumType" use="optional" name="compression">
            <xs:annotation>
              <xs:documentation>The compression to use for the transport.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/cassandradb" type="booleanType" use="optional" default="false" name="sslEnabled">
            <xs:annotation>
              <xs:documentation>Enables the use of SSL for the cluster.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="username">
            <xs:annotation>
              <xs:documentation>A username. NOTE: Please use a dummy username if you have disabled authentication</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" name="password">
            <xs:annotation>
              <xs:documentation>A password. NOTE: Leave empty if not required. If specified, the connector will try to login with this credentials</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Create-keyspaceType" substitutionGroup="mule:abstract-message-processor" name="create-keyspace">
    <xs:annotation>
      <xs:documentation>Creates a new keyspace</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Create-keyspaceType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="input-ref">
          <xs:annotation>
            <xs:documentation>operation input containing the keyspace name and the replication details</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Drop-keyspaceType" substitutionGroup="mule:abstract-message-processor" name="drop-keyspace">
    <xs:annotation>
      <xs:documentation>Drops the entire keyspace</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Drop-keyspaceType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>the name of the keyspace to be dropped</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Create-tableType" substitutionGroup="mule:abstract-message-processor" name="create-table">
    <xs:annotation>
      <xs:documentation>Creates a table(column family) in a specific keyspace; If no keyspace is specified the keyspace used for login will be used</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Create-tableType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="input-ref">
          <xs:annotation>
            <xs:documentation>operation input describing the table name, the keyspace name and the list of columns</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Drop-tableType" substitutionGroup="mule:abstract-message-processor" name="drop-table">
    <xs:annotation>
      <xs:documentation>Drops an entire table form the specified keyspace or from the keyspace used for login if none is specified as an operation parameter</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Drop-tableType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="tableName">
          <xs:annotation>
            <xs:documentation>the name of the table to be dropped</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be dropped</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Execute-c-q-l-queryType" substitutionGroup="mule:abstract-message-processor" name="execute-c-q-l-query">
    <xs:annotation>
      <xs:documentation>Executes the raw input query provided</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Execute-c-q-l-queryType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="input-ref">
          <xs:annotation>
            <xs:documentation>CQLQueryInput describing the parametrized query to be executed along with the parameters</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="InsertType" substitutionGroup="mule:abstract-message-processor" name="insert">
    <xs:annotation>
      <xs:documentation>Executes the insert entity operation</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="InsertType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the table name in which the entity will be inserted</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="entity-ref">
          <xs:annotation>
            <xs:documentation>the entity to be inserted</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="UpdateType" substitutionGroup="mule:abstract-message-processor" name="update">
    <xs:annotation>
      <xs:documentation>Executes the update entity operation</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="UpdateType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the table name in which the entity will be updated</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be dropped</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="entity-ref">
          <xs:annotation>
            <xs:documentation>the entity to be updated</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Delete-columns-valueType" substitutionGroup="mule:abstract-message-processor" name="delete-columns-value">
    <xs:annotation>
      <xs:documentation>Deletes values from an object specified by the where clause</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Delete-columns-valueType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the name of the table</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="payload-ref">
          <xs:annotation>
            <xs:documentation>operation input: columns to be deleted and where clause for the delete operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Delete-rowsType" substitutionGroup="mule:abstract-message-processor" name="delete-rows">
    <xs:annotation>
      <xs:documentation>Deletes an entire record</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Delete-rowsType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the name of the table</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="payload-ref">
          <xs:annotation>
            <xs:documentation>operation input: where clause for the delete operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="SelectType" substitutionGroup="mule:abstract-message-processor" name="select">
    <xs:annotation>
      <xs:documentation>Executes a select query</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="ObjectObjectType">
    <xs:sequence></xs:sequence>
    <xs:attribute type="xs:string" use="optional" name="ref">
      <xs:annotation>
        <xs:documentation>The reference object for this parameter</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="SelectType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="parameters">
            <xs:annotation>
              <xs:documentation>the query parameters</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="parameter">
                  <xs:complexType>
                    <xs:complexContent>
                      <xs:extension xmlns="http://www.mulesoft.org/schema/mule/cassandradb" base="ObjectObjectType">
                        <xs:attribute type="xs:string" use="optional" name="value-ref"></xs:attribute>
                      </xs:extension>
                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute type="xs:string" use="optional" name="ref">
                <xs:annotation>
                  <xs:documentation>The reference object for this parameter</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="query">
          <xs:annotation>
            <xs:documentation>the query to be executed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Get-table-names-from-keyspaceType" substitutionGroup="mule:abstract-message-processor" name="get-table-names-from-keyspace">
    <xs:annotation>
      <xs:documentation>Returns all the table names from the specified keyspace</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Get-table-names-from-keyspaceType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>the name of the keyspace to be used on the operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Change-column-typeType" substitutionGroup="mule:abstract-message-processor" name="change-column-type">
    <xs:annotation>
      <xs:documentation>Changes the type of a column - check compatibility here: &lt;a href="http://docs.datastax.com/en/cql/3.1/cql/cql_reference/cql_data_types_c.html#concept_ds_wbk_zdt_xj__cql_data_type_compatibility"&gt;CQL type compatibility&lt;/a&gt;</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Change-column-typeType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the name of the table to be used for the operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="input-ref">
          <xs:annotation>
            <xs:documentation>POJO defining the name of the column to be changed and the new DataType</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Add-new-columnType" substitutionGroup="mule:abstract-message-processor" name="add-new-column">
    <xs:annotation>
      <xs:documentation>Adds a new column</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Add-new-columnType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the name of the table to be used for the operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="input-ref">
          <xs:annotation>
            <xs:documentation>POJO defining the name of the new column and its DataType</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Drop-columnType" substitutionGroup="mule:abstract-message-processor" name="drop-column">
    <xs:annotation>
      <xs:documentation>Removes a column</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Drop-columnType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="table">
          <xs:annotation>
            <xs:documentation>the name of the table to be used for the operation</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="keyspaceName">
          <xs:annotation>
            <xs:documentation>(optional) the keyspace which contains the table to be used</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" default="#[payload]" name="columnName">
          <xs:annotation>
            <xs:documentation>the name of the column to be removed</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/cassandradb" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Rename-columnType" substitutionGroup="mule:abstract-message-processor" name="rename-column">
    <xs:annotation>
      <xs:documentation>Renames a column</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Rename-columnType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageProcessorType">
